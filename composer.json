{
  "name": "mk/form",
  "description": "A powerful form backend system for Laravel with fluent API, validation, and HTTP endpoints",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Malik Krehic",
      "email": "malik.krehic@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^12.0",
    "illuminate/http": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/validation": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Mk\\Form\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mk\\Form\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mk\\Form\\FormServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
